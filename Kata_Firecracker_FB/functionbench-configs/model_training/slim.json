{
  "metadata": {
    "name": "model_training_slim-pod"
  },
  "image": {
    "image": "ghcr.io/arkountos/model_training_slim:latest"
  },
  "command": ["python3","invoke.py"],
  "args": ["functionbench-training-data-in", "dataset_blob", "functionbench-training-model-in", "model_blob", "10"],
  "envs": [
    { "key": "MINIO_URL", "value": "10.0.2.15:9000" },
    { "key": "MINIO_ACCESS_KEY", "value": "minioadmin" },
    { "key": "MINIO_SECRET_KEY", "value": "minioadmin" },
    { "key": "MINIO_SECURE", "value": "false" },
    { "key": "MINIO_REGION", "value": "us-east-1" },
    { "key": "MINIO_DELTA_TIME", "value": "0" }
  ],
  "log_path": "model_training_slim-container.log",
  "linux": {
    "security_context": {
      "privileged": false,
      "capabilities": {
        "add_capabilities": [
          "SYS_TIME"
        ]
      }
    }
  }
}
